<Window x:Class="Uvelir.Views.Windows.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uvelir.Views.Windows"
        mc:Ignorable="d"
        Title="AdminWindow" Height="600" Width="1080">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70">
            </RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal" >
            <TextBlock Name="tblNameUser"
                   VerticalAlignment="Top"
                   HorizontalAlignment="Left"
                   Margin="10"
                   FontSize="14"/>
        </StackPanel>
        <Grid Grid.Row="1">
            <Frame Name="AdminFrame" NavigationUIVisibility="Hidden"></Frame>
            
        </Grid>

       
    </Grid>
</Window>
